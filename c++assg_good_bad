#include<iostream>
using namespace std;
int main()
{
    int m;
    cout<<"Enter the number to check weather good number or not"<<endl;
    cin>>m;
    if(m%3==0 && m%5==0)
        cout<<"Good Number"<<endl;

    else if(m%3==0 && m%5!=0)
        cout<<"Bad number"<<endl;

    else if(m%3!=0 && m%5==0)
        cout<<"Poor number"<<endl;

    else{
        cout<<"-1"<<endl;

    }
    int n,c=0;
    cout<<"Enter the number to reverse"<<endl;
    cin>>n;
    while(n>0)
    {
        c=c*10+n%10;
        n=n/10;
    }
    cout<<c;
    return 0;
}
