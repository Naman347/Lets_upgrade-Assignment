#include<iostream>
using namespace std;
int main()
{
    int n;
    cout<<"Enter the number to checker weather even or odd"<<endl;
    cin>>n;
    if(n%2==0)
        cout<<"even"<<endl;
    else
        cout<<"odd"<<endl;
    cout<<"program to swap two numbers without using 3rd variable"<<endl;
    int a,b;
    cout<<"Enter the two numbers"<<endl;
    cin>>a>>b;
    swap(a,b);
    cout<<a<<endl<<b;

}
