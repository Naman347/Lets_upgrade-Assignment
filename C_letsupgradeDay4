#include<stdio.h>
int main()
{
    int m,flag=0;
    printf("Enter the number::");
    scanf("%d",&m);
    int n=m/2;
    for(int i=2;i<n;i++)
    {
        if(m%i==0)
        {
            printf("Not a prime number");
            flag=1;
            break;
        }
    }
    if(flag==0)
        printf("Number is prime");
}
